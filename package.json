{
  "name": "yaml-symbols",
  "displayName": "yaml-symbols",
  "description": "Provide Symbols for Yaml files",
  "version": "0.0.3",
  "publisher": "Cronos87",
  "author": "Yohan T.",
  "homepage": "https://github.com/Cronos87/vsc-yaml-symbols",
  "bugs": {
        "url": "https://github.com/Cronos87/vsc-yaml-symbols/issues"
    },
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Languages"
  ],
  "keywords": [
    "symbols",
    "yaml"
  ],
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}
